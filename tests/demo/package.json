{
	"name": "demo",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "VITE_COVERAGE=true vite build",
		"build:swa": "PUBLIC_SWA=true npm run build",
		"preview": "vite preview",
		"swa": "PUBLIC_SWA=true swa start",
		"prepare": "../../scripts/npm-link.sh && (svelte-kit sync || echo '')",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:swa": "PUBLIC_SWA=true npm run test"
	},
	"devDependencies": {
		"@azure/static-web-apps-cli": "^2.0.6",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"nyc": "^17.1.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.0",
		"svelte-adapter-azure-swa": "file:../..",
		"svelte-check": "^4.1.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vite-plugin-istanbul": "^7.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@sentry/sveltekit": "^9.15.0"
	},
	"engines": {
		"node": ">=20.0.0 <21"
	}
}
