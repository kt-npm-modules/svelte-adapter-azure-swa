{
	"name": "demo",
	"private": true,
	"version": "0.0.2",
	"type": "module",
	"scripts": {
		"dev": "PUBLIC_SWA=${PUBLIC_SWA:-false} vite dev",
		"swa": "PUBLIC_SWA=true swa start",
		"build": "PUBLIC_SWA=${PUBLIC_SWA:-false} vite build",
		"build:swa": "PUBLIC_SWA=true npm run build",
		"preview": "PUBLIC_SWA=${PUBLIC_SWA:-false} vite preview",
		"prepare": "PUBLIC_SWA=${PUBLIC_SWA:-false} svelte-kit sync || echo ''",
		"check": "PUBLIC_SWA=${PUBLIC_SWA:-false} svelte-kit sync && PUBLIC_SWA=${PUBLIC_SWA:-false} svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:swa": "PUBLIC_SWA=true npm run test"
	},
	"devDependencies": {
		"@azure/functions": "^4.7.2",
		"@azure/static-web-apps-cli": "^2.0.6",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@inlang/paraglide-js": "^2.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-node": "^5.3.2",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^24",
		"@vitest/browser": "^4.0.8",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.25.0",
		"svelte-adapter-azure-swa": "file:../..",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@azure/cosmos": "^4.5.1",
		"@sentry/sveltekit": "^10.15.0"
	},
	"engines": {
		"node": ">=20 <21"
	}
}
