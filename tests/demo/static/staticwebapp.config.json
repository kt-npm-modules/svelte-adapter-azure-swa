{
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/api/*"
    },
    {
      "route": "/data-api/*"
    },
    {
      "route": "*",
      "methods": [
        "POST",
        "PUT",
        "DELETE"
      ],
      "rewrite": "/api/sk_render"
    },
    {
      "route": "/_app/immutable/*",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "route": "/index.html",
      "rewrite": "/api/sk_render"
    },
    {
      "route": "/",
      "rewrite": "/api/sk_render"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/sk_render"
  }
}