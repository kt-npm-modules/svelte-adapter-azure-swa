{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"noImplicitAny": true,
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"lib": ["es2023"]
	},
	"include": [
		"index.js",
		"index.d.ts",
		"rollup-client.js",
		"rollup-server.js",
		"entry",
		"files",
		"types"
	]
}
